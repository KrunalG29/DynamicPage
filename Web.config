		<add name="ConStr" connectionString="Data Source=192.168.30.167;Initial Catalog=CMS_EXAM_NEW;Integrated Security=False; User Id=krunalg; Password=krunalg;Max Pool Size = 1000" providerName="System.Data.SqlClient"/>

		<!--<add name="ConStr" connectionString="Data Source=4.240.123.71;Initial Catalog=infiprod_vrl;User ID=sa;Password=GpIC4yVNAwZjFFv;Application Name=iERP" providerName="System.Data.SqlClient"/>-->
		<!--<add name="ConStr" connectionString="Data Source=DESKTOP-9OIC7LF;Initial Catalog=test;Integrated Security=True" providerName="System.Data.SqlClient"/>-->
